<view class="head" style="height:{{HeadBar}}rpx;">
  <view class="titles">
    <view class="title">
      KeLi
    </view>
    <view class="sudtitle">
      副标题
    </view>
  </view>
</view>
<view class="back">
  <view class="clouds">
    <image animation="{{animationCloudData}}" class="yun1"
      src="cloud://cloudtest-3g82y8a0d914b437.636c-cloudtest-3g82y8a0d914b437-1311354097/home/cloud.png"
      style="position: relative; left: 121rpx; top: 374rpx; opacity: 0.4"></image>
  </view>
  <view class="clouds">
    <image animation="{{animationCloudData}}" class="yun2"
      src="cloud://cloudtest-3g82y8a0d914b437.636c-cloudtest-3g82y8a0d914b437-1311354097/home/cloud.png"
      style="position: relative; left: 118rpx; top: -394rpx; opacity: 0.4"></image>
  </view>
  <view class="clouds">
    <image animation="{{animationCloudData}}" class="yun3"
      src="cloud://cloudtest-3g82y8a0d914b437.636c-cloudtest-3g82y8a0d914b437-1311354097/home/cloud.png"
      style="position: relative; left: 100rpx; top: -234rpx; opacity: 0.4"></image>
  </view>

  <swiper class="screen-swiper {{DotStyle?'square-dot':'round-dot'}}" indicator-dots="true" circular="true"
  autoplay="true" interval="5000" duration="500" style="height: 200rpx; position: relative; left: 0rpx; top: -1416rpx">
  <swiper-item wx:for="{{swiperList}}" wx:key style="position: relative; left: 0rpx; top: 0rpx">
    <image src="{{item.url}}" mode="aspectFill" wx:if="{{item.type=='image'}}"></image>
    <video src="{{item.url}}" autoplay loop muted show-play-btn="{{false}}" controls="{{false}}" objectFit="cover"
      wx:if="{{item.type=='video'}}"></video>
  </swiper-item>
</swiper>

